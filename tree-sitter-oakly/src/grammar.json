{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "oakly",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "type_definition"
          },
          {
            "type": "SYMBOL",
            "name": "value_definition"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "type_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "sum_type"
            },
            {
              "type": "SYMBOL",
              "name": "product_type"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "sum_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "type"
        },
        {
          "type": "SYMBOL",
          "name": "type_identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "type_parameter"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "constructor"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "|"
              },
              {
                "type": "SYMBOL",
                "name": "constructor"
              }
            ]
          }
        }
      ]
    },
    "constructor": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "constructor_name"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "type_identifier"
              },
              {
                "type": "SYMBOL",
                "name": "type_parameter"
              }
            ]
          }
        }
      ]
    },
    "constructor_name": {
      "type": "PATTERN",
      "value": "[A-Z][A-Za-z]*"
    },
    "type_identifier": {
      "type": "PATTERN",
      "value": "[A-Z][A-Za-z]*"
    },
    "type_parameter": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "value_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "value_identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "parameter"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        }
      ]
    },
    "parameter": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "value_identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "product_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "alias"
        },
        {
          "type": "SYMBOL",
          "name": "type_identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "type_parameter"
          }
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "FIELD",
          "name": "include",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "SYMBOL",
                    "name": "type_parameter"
                  },
                  {
                    "type": "STRING",
                    "value": "|"
                  }
                ]
              },
              {
                "type": "BLANK"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "name_type_pair"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": ","
              },
              {
                "type": "SYMBOL",
                "name": "name_type_pair"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "name_type_pair": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field_identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type_identifier"
        }
      ]
    },
    "field_identifier": {
      "type": "PATTERN",
      "value": "[a-z]+"
    },
    "expression": {
      "type": "SYMBOL",
      "name": "string"
    },
    "string": {
      "type": "PATTERN",
      "value": "\\\".*\\\""
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "[ \\t\\n\\r]"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [
    []
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
